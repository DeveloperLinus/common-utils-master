<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="submit" />

        <Button
            android:id="@+id/btn_increment_up_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="监听增量更新" />

        <Button
            android:id="@+id/btn_record_up_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="监听通行记录上传" />

        <Button
            android:id="@+id/btn_remove_increment_up"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="移除增量数据上传监听" />

        <Button
            android:id="@+id/btn_remove_record_up"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="移除通行记录上传监听" />
    </LinearLayout>
</layout>